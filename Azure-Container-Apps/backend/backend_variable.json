{
  "subscription": {
    "id": ""
  },
  "resourceGroup": {
    "name": "SALES"
  },
  "keyVault": {
    "name": "backend-web-rg-kv-2"
  },
  "acr": {
    "server": "acr.azurecr.io",
    "username": "acrusername"
  },
  "managedIdentities": {
    "runtime": "sales-reader-mi",
    "deployment": "sales-deployment-mi"
  },
  "containerApp": {
    "name": "backend-sales",
    "location": "westeurope",
    "managedEnvironmentName": "deployment-ca-environment",
    "imageName": "",
    "targetPort": 8001,
    "ingress": {
      "enabled": true,
      "external": false,
      "transport": "auto",
      "allowInsecure": false
    },
    "resources": {
      "cpu": 1,
      "memory": "2Gi"
    },
    "scaling": {
      "minReplicas": 1,
      "maxReplicas": 2
    }
  },
  "volumes": [
    {
      "name": "backend-sales",
      "storageName": "backend-sales",
      "mountPath": "/config"
    }
  ],
  "environmentVariables": {
    "secrets": {
      "JWT_SECRET_KEY": {
        "keyVaultSecret": "jwt-secret-key"
      }
    },
    "nonSecrets": {
      "URL": "0.0.0.0",
      "ENV": "PRD"
    }
  },
  "startupCommand": {
    "command": "/bin/sh",
    "args": [
      "-c",
      "mkdir -p abc"
    ]
  }
}
